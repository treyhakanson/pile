{
  "manifest_version": 2,

  "name": "OSU CSE Styler",
  "description": "This extension makes the OSU CSE pages not look like ass",
  "version": "1.0",

  "browser_action": {
    // "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Style Page"
  },

  "permissions": [
    "activeTab"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ]
}
