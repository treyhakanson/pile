{
  "manifest_version": 2,

  "name": "Playback Rate",
  "description": "This extension speeds up video playback on new masters academy",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Speed Up Playback"
  },

  "permissions": [
    "activeTab"
  ],

  "content_scripts": [
    {
      "matches": ["*://www.newmastersacademy.org/*"],
      "js": ["script.js"]
    }
  ]
}
